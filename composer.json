{
    "name": "pandawa/pgbouncer",
    "description": "PgBouncer support for pandawa",
    "keywords": [
        "laravel",
        "pandawa",
        "pgbouncer"
    ],
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*",
        "pandawa/foundation-bundle": "^5.0",
        "pandawa/database-bundle": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\PgBouncer\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\PgBouncer\\PgBouncerBundle"
            ]
        }
    }
}
